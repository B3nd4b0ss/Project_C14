<Page x:Class="Project_C14.Erweiterung"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Project_C14"
      mc:Ignorable="d"
      Title="Erweiterung" Height="500" Width="1200">
    <Grid>
        <Border Style="{StaticResource MainBorder}" Margin="30 0">
            <StackPanel>
                <TextBlock x:Name="Text1"></TextBlock>
                <TextBlock x:Name="Text2"></TextBlock>
                <TextBlock x:Name="Text3"></TextBlock>
                <TextBlock x:Name="Text4"></TextBlock>
                <TextBlock x:Name="Text5"></TextBlock>
                <TextBlock x:Name="Text6"></TextBlock>
                <TextBlock x:Name="Text7"></TextBlock>
                <Button x:Name="Berechnen"></Button>
            </StackPanel>
        </Border>
    </Grid>
</Page>
