<Page x:Class="Project_C14.Account"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Project_C14"
      mc:Ignorable="d"
      Title="Library" Height="500" Width="1200">
    <Grid>
        <Border Style="{StaticResource MainBorder}" Margin="30 0"></Border>
        <StackPanel>
            <Frame x:Name="AccountFrame" NavigationUIVisibility="Hidden" />
        </StackPanel>
    </Grid>
</Page>